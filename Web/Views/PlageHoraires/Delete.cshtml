@model Microservice.Models.PlageHoraire

@{
    ViewData["Title"] = "Supprimer - Plage horaire";
}

<h1>Supprimer</h1>

<h3>Êtes-vous sûr de vouloir supprimer?</h3>
<div>
    <h4>PlageHoraire</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Date)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HeureDebut)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HeureDebut)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HeureTer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HeureTer)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ActeMedical)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ActeMedical.Libelle)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Specialite)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Specialite.Nom)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input class="btn btn-danger" type="submit" value="Supprimer" /> |
        <a class="btn btn-success" asp-action="Index">Retour a la liste</a>
    </form>
</div>
